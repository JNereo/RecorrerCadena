public class RecorridoCadena {

    public static void main(String[] args) {
        String cadena = "Hola Mundo";
        
        // Recorremos la cadena
        System.out.println("-----------");
        for (int i = 0; i < cadena.length(); i++) {
            char caracter = cadena.charAt(i);
            System.out.println(caracter);
     //fin de nuevo
        }
}}
